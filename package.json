{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "tailwindcss -i ./src-new/styles.css -o ./styles.css && node esbuild.config.mjs",
		"build": "tailwindcss -i ./src-new/styles.css -o ./styles.css --minify && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "node --test test/**/*.test.js",
		"test:watch": "node --test --watch test/**/*.test.js",
		"test:coverage": "node --test --experimental-test-coverage test/**/*.test.js"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@fal-ai/serverless-client": "^0.9.0",
		"@ffmpeg/core": "^0.12.10",
		"ag-psd": "^28.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"replicate": "^0.25.0",
		"spectral.js": "^3.0.0",
		"zustand": "^5.0.5"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.4",
		"esmock": "^2.6.0",
		"obsidian": "latest",
		"sinon": "^17.0.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.0",
		"typescript": "^5.0.0"
	}
}
