{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "tailwindcss -i ./src/styles.css -o ./styles.css && node esbuild.config.mjs",
		"build": "tailwindcss -i ./src/styles.css -o ./styles.css --minify && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@ffmpeg/core": "^0.12.10",
		"ag-psd": "^28.0.1",
		"openai": "^4.98.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.25.4",
		"obsidian": "latest",
		"tailwindcss": "^3.4.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
